{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "gcc_11.3",
			"command": "C:\\mingw64\\bin\\g++.exe",
			"args": [
				"-ggdb",
				"-std=c++17",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: clang++.exe",
			"command": "F:\\llvm_x64_gnu\\bin\\clang++.exe",
			"args": [
				"-g",
				"${file}",
				"-std=c++17",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "F:\\llvm_x64_gnu\\bin\\"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "编译器: \"F:\\llvm_x64_gnu\\bin\\clang++.exe\""
		},
	]
}